<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context="com.example.pn748_000.lolinfo.BlankFragment"
 >
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">



    <LinearLayout

        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:id="@+id/one">




        <LinearLayout
       android:layout_width="match_parent"
       android:layout_height="wrap_content"
       android:padding="10dp"
       android:orientation="vertical"
       >
       <LinearLayout
           android:layout_width="match_parent"
           android:layout_height="wrap_content"
           android:orientation="horizontal">
       <ImageView
           android:id="@+id/icon_container"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:layout_margin="20dp"


           />

       <TextView

           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:textSize="30sp"

           android:id="@+id/name_text"
           android:layout_margin="10dp"/>
</LinearLayout>
       <LinearLayout
           android:layout_width="match_parent"
           android:layout_height="wrap_content"
           android:orientation="horizontal">
           <TextView
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:id="@+id/region_text"

               android:textSize="20sp"
               android:layout_margin="5dp"/>
       <TextView

           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:textSize="20sp"

           android:id="@+id/level_text"
           android:layout_margin="5dp"/>

       </LinearLayout>
       <TextView


           android:layout_toRightOf="@id/level_text"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:textSize="20sp"


           android:layout_margin="5dp"/>
   </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:weightSum="3"
            android:id="@+id/leaguesLayout"
            android:alpha="0">
       <LinearLayout
           android:layout_width="0dp"
           android:layout_weight="1"
           android:layout_height="wrap_content"
           android:gravity="center_horizontal"
           android:orientation="vertical">
           <TextView
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:text="Team 3v3"/>
           <ImageView
               android:id="@+id/img_3v3"
               android:layout_width="wrap_content"
               android:layout_height="100dp"
               android:src="@drawable/provisional"/>
           <TextView
               android:id="@+id/rank_text_3v3"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:text="Unranked"/>
           <TextView
               android:id="@+id/lp_3v3"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"

               android:textSize="@dimen/lp_size"/>
           <TextView
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:id="@+id/wins_loses_3v3"
               android:textSize="@dimen/lp_size"/>

       </LinearLayout>
            <LinearLayout
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:orientation="vertical">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Solo 5v5"/>
                <ImageView
                    android:id="@+id/img_5v5"
                    android:layout_width="wrap_content"
                    android:layout_height="100dp"
                    android:src="@drawable/provisional"/>
                <TextView
                    android:id="@+id/rank_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Unranked"
                    />
                <TextView
                    android:id="@+id/lp_5v5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"

                    android:textSize="@dimen/lp_size"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/wins_loses_5v5"
                    android:textSize="@dimen/lp_size"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:orientation="vertical">
                <TextView

                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Team 5v5"/>
                <ImageView
                    android:id="@+id/img_5v5_t"
                    android:layout_width="wrap_content"
                    android:layout_height="100dp"
                    android:src="@drawable/provisional"/>
                <TextView
                    android:id="@+id/rank_text_5v5_t"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Unranked"/>
                <TextView
                    android:id="@+id/lp_5v5_t"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/lp_size"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/wins_loses_5v5_t"
                    android:textSize="@dimen/lp_size"/>
            </LinearLayout>
        </LinearLayout>
        </LinearLayout>

    </RelativeLayout>
</FrameLayout>
