<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context="com.pnapps.pn748_000.PlayersOfLoL.FreeChampFragment"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"
    android:clickable="true">
<LinearLayout
    android:layout_height="match_parent"


    android:background="@color/darkGrey"

    android:layout_width="match_parent"
    android:alpha=".9"
    />


    <GridLayout
        android:layout_gravity="center"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:columnCount="5"
        android:rowCount="2">
        <ImageView
            android:layout_width="62dp"
            android:layout_height="112dp"


            android:id="@+id/champ1"/>
        <ImageView
            android:layout_width="62dp"
            android:layout_height="112dp"


            android:id="@+id/champ2"/>
        <ImageView
            android:layout_width="62dp"
            android:layout_height="112dp"


            android:id="@+id/champ3"/>
        <ImageView
            android:layout_width="62dp"
            android:layout_height="112dp"

            android:id="@+id/champ4"/>
        <ImageView
            android:layout_width="62dp"
            android:layout_height="112dp"
            android:id="@+id/champ5"/>
        <ImageView
            android:layout_width="62dp"
            android:layout_height="112dp"
            android:id="@+id/champ6"/>
        <ImageView
            android:layout_width="62dp"
            android:layout_height="112dp"
            android:id="@+id/champ7"/>
        <ImageView
            android:layout_width="62dp"
            android:layout_height="112dp"
            android:id="@+id/champ8"/>
        <ImageView
            android:layout_width="62dp"
            android:layout_height="112dp"
            android:id="@+id/champ9"/>
        <ImageView
            android:layout_width="62dp"
            android:layout_height="112dp"
            android:id="@+id/champ10"/>
    </GridLayout>
</FrameLayout>
